<html>
  <head>
    <title>Registered Users</title>
    <link rel="stylesheet" href="/styles.css"> <!-- Link to the CSS file -->
  </head>
  <body>
    <h1>Registered Users</h1>
    <ul>
      {{#each users}}
        <li>{{this.username}} - {{this.email}}</li>
      {{/each}}
    </ul>
    <a href="/">Back to Registration</a>
    <h4>{{pod_name}}</h4>
  </body>

</html>
